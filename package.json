{
  "name": "webpack-vue",
  "version": "1.0.0",
  "description": "",
  "repository": "",
  "scripts": {
    "tsc": "tsc --watch",
    "rebuild": "electron-rebuild",
    "prod:renderer:analyzer": "webpack --env mode=production --analyze --progress --config build/webpack.renderer.config.js ",
    "prod:main:analyzer": "webpack --env mode=production --analyze --progress --config build/webpack.main.config.js",
    "prod": "webpack --env mode=production --progress --config build/webpack.renderer.config.js && webpack --env mode=production --progress --config build/webpack.main.config.js",
    "dev": "node ./build/dev.js --env mode=development --progress",
    "build": "node ./build/build.js --env mode=production --progress"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "build": {
    "asar": true,
    "productName": "electron-vue",
    "appId": "com.electron-vue.template",
    "copyright": "Copyright Â© electron-vue-template",
    "directories": {
      "output": "dist/"
    },
    "files": [
      "main/**",
      "renderer/**"
    ],
    "win": {
      "icon": "./logo.ico",
      "artifactName": "${productName}_${version}.${ext}",
      "verifyUpdateCodeSignature": false,
      "target": [
        {
          "target": "nsis",
          "arch": [
            "ia32"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "createDesktopShortcut": "always",
      "deleteAppDataOnUninstall": true,
      "allowToChangeInstallationDirectory": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/plugin-proposal-class-properties": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.17.0",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/runtime-corejs3": "^7.17.9",
    "@vue/babel-plugin-jsx": "^1.1.1",
    "@vue/compiler-sfc": "^3.2.33",
    "autoprefixer": "^10.4.5",
    "babel-loader": "^8.2.5",
    "chalk": "5",
    "clean-webpack-plugin": "^4.0.0",
    "crocket": "^1.0.15",
    "css-loader": "^6.7.1",
    "css-url-relative-plugin": "^1.1.0",
    "cssnano": "^5.1.7",
    "electron": "^18.1.0",
    "electron-builder": "^23.0.3",
    "electron-rebuild": "^3.2.7",
    "html-loader": "^3.1.0",
    "html-webpack-plugin": "^5.5.0",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "mini-css-extract-plugin": "^2.6.0",
    "portfinder": "^1.0.28",
    "postcss": "^8.4.12",
    "postcss-loader": "^6.2.1",
    "sass": "^1.50.1",
    "sass-loader": "^12.6.0",
    "sass-resources-loader": "^2.2.5",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "thread-loader": "^3.0.4",
    "typescript": "^4.6.3",
    "vue-loader": "^17.0.0",
    "webpack": "^5.72.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1",
    "webpack-merge": "^5.8.0"
  },
  "dependencies": {
    "vue": "^3.2.33",
    "vue-router": "^4.0.14",
    "vuex": "^4.0.2"
  }
}
