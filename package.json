{
  "name": "webpack-vue",
  "version": "1.0.0",
  "description": "",
  "repository": "",
  "scripts": {
    "tsc": "tsc --watch",
    "postinstall": "electron-builder install-app-deps",
    "rebuild": "electron-rebuild",
    "prod:renderer:analyzer": "webpack --env mode=production --analyze --progress --config build/webpack.renderer.config.js ",
    "prod:main:analyzer": "webpack --env mode=production --analyze --progress --config build/webpack.main.config.js",
    "prod": "webpack --env mode=production --progress --config build/webpack.renderer.config.js && webpack --env mode=production --progress --config build/webpack.main.config.js",
    "dev": "node ./build/dev.js --env mode=development --progress",
    "build": "node ./build/build.js --env mode=production --progress"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "build": {
    "asar": true,
    "productName": "electron-vue",
    "appId": "com.electron-vue.template",
    "copyright": "Copyright Â© electron-vue-template",
    "directories": {
      "output": "dist/"
    },
    "files": [
      "main/**",
      "renderer/**"
    ],
    "win": {
      "icon": "logo.ico",
      "artifactName": "${productName}_${version}.${ext}",
      "verifyUpdateCodeSignature": false,
      "target": [
        {
          "target": "nsis",
          "arch": [
            "ia32"
          ]
        }
      ]
    },
    "nsis": {
      "oneClick": false,
      "perMachine": true,
      "createDesktopShortcut": "always",
      "deleteAppDataOnUninstall": true,
      "allowToChangeInstallationDirectory": true
    }
  },
  "devDependencies": {
    "@babel/core": "^7.12.17",
    "@babel/plugin-transform-runtime": "^7.12.17",
    "@babel/preset-env": "^7.12.17",
    "@babel/preset-typescript": "^7.12.17",
    "@babel/runtime-corejs3": "^7.12.18",
    "@vue/babel-plugin-jsx": "^1.0.3",
    "@vue/compiler-sfc": "^3.0.5",
    "autoprefixer": "^10.2.4",
    "babel-loader": "^8.2.2",
    "chalk": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "crocket": "^0.9.11",
    "css-loader": "^5.0.2",
    "css-url-relative-plugin": "^1.0.1",
    "cssnano": "^4.1.10",
    "dart-sass": "^1.25.0",
    "electron": "^11.3.0",
    "electron-builder": "^22.9.1",
    "electron-rebuild": "^2.3.5",
    "html-loader": "^2.0.0",
    "html-webpack-plugin": "^5.2.0",
    "less": "^4.1.1",
    "less-loader": "^8.0.0",
    "mini-css-extract-plugin": "^1.3.8",
    "portfinder": "^1.0.28",
    "postcss": "^8.2.6",
    "postcss-loader": "^5.0.0",
    "sass": "^1.32.8",
    "sass-loader": "^11.0.1",
    "sass-resources-loader": "^2.1.1",
    "style-loader": "^2.0.0",
    "thread-loader": "^3.0.1",
    "typescript": "^4.1.5",
    "vue-loader": "^16.1.0",
    "webpack": "^5.23.0",
    "webpack-bundle-analyzer": "^4.4.0",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2",
    "webpack-merge": "^5.7.3"
  },
  "dependencies": {
    "vue": "^3.0.5",
    "vue-router": "^4.0.3",
    "vuex": "^4.0.0"
  }
}
